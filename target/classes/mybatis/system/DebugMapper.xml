<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="DebugMapper">

	<!-- 新增 -->
	<insert id="save" parameterType="pd">
		insert into debug(
			order_id,
			status
		) values
		(
			#{order_id},
			#{status}
		)
	</insert>

</mapper>